{
  "name": "aragog",
  "version": "1.1.0",
  "description": "Distributed web scraping framework",
  "author": "Fei Liu",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src --exec node --inspect -r ts-node/register ./src/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.server.json && cp src/headless/jquery*.js dist/headless",
    "init-docker": "rimraf docker/aragog-server/package.json docker/aragog-server/yarn.lock docker/aragog-server/dist && cp -r dist package.json yarn.lock docker/aragog-server",
    "build:client": "rimraf client/lib && tsc -d -p tsconfig.client.json && node script/after-client-build.js",
    "server": "node dist/main.js",
    "lint": "tsc --noEmit -p tsconfig.server.json && tsc --noEmit -p tsconfig.client.json && eslint --ext .ts ./src ./client ./test",
    "test": "jest --verbose --forceExit",
    "test:cov": "yarn test --coverage",
    "upload-cov": "codecov --disable=gcov"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/cheerio": "^0.22.21",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.158",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "codecov": "^3.8.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^10.0.0",
    "iconv-lite": "^0.6.2",
    "json5": "^2.2.2",
    "lodash": "^4.17.19",
    "log4js": "^6.3.0",
    "puppeteer": "^11.0.0",
    "vm2": "^3.9.2"
  }
}
